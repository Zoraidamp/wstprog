zad.1

 #include <stdio.h> 
 void main()
 {
  int a,b,c;
  double delta;
  printf("program ten liczy deltę funkcji kwadratowej, podaj czynniki: \n ax^2+bx+c \n (a,b,c):");
  scanf("%d%d%d",&a,&b,&c);
  delta=b*b-4*a*c;
  printf("delta wynosi: %lf \n", delta); 
}

zad.2s

 #include <stdio.h> 
 void main()
 {
  int x,y;
  printf("Napisać i uruchomić program w C, który wczytuje dwie liczby rzeczywiste x i y, a następnie drukuje informację, jaki kolor na poniższym rysunku ma punkt o współrzędnych (x, y) \n podaj x i y:");
  scanf("%d%d",&x,&y);
  if (x+y <= 6) printf("punkt ten jest biały \n");
 else printf ("punkt ten jest czarny \n");
}
